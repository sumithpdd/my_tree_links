{
    "name": "MylinkTree Extension",
    "description": "My Link Tree extension",
    "version": "1.1.0",
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["contextMenus", "clipboardWrite", "clipboardRead"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/mytreelinks16.png",
            "32": "icons/mytreelinks32.png",
            "48": "icons/mytreelinks48.png",
            "128": "icons/mytreelinks128.png"
          }
    },
    "icons": {
      "16": "icons/mytreelinks16.png",
      "32": "icons/mytreelinks32.png",
      "48": "icons/mytreelinks48.png",
      "128": "icons/mytreelinks128.png"
    },
    "manifest_version": 3
}